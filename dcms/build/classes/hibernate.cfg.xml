<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/dcms</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.connection.password">123456</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="hibernate.show_sql">true</property>
        <mapping resource="com/nxdcms/entity/Subcompetition.hbm.xml" />
        <mapping resource="com/nxdcms/entity/News.hbm.xml" />
        <mapping resource="com/nxdcms/entity/Notice.hbm.xml" />
        <mapping resource="com/nxdcms/entity/Competition.hbm.xml" />
        <mapping resource="com/nxdcms/entity/Administrator.hbm.xml" />
        <mapping resource="com/nxdcms/entity/Student.hbm.xml" />
        <mapping resource="com/nxdcms/entity/UserPerference.hbm.xml" />
        <mapping resource="com/nxdcms/entity/collegedictionary.hbm.xml" />
        <mapping resource="com/nxdcms/entity/competitiondictionary.hbm.xml" />
        <mapping resource="com/nxdcms/entity/compLeveldictionary.hbm.xml" />
         <mapping resource="com/nxdcms/entity/dictionaryManage.hbm.xml" />
    </session-factory>
</hibernate-configuration>
